{
    "name": "chelout/ozla-php-sdk",
    "description": "An SDK to easily work with the Ozla API",
    "keywords": [
        "ozla",
        "api",
        "sdk"
    ],
    "homepage": "https://ozla.ru",
    "license": "MIT",
    "authors": [
        {
            "name": "Viacheslav Ostrovskiy",
            "email": "chelout@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Chelout\\PhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chelout\\PhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
